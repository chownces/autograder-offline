{
  "name": "grader",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "typecheck": "tsc --noEmit",
    "test": "cross-env TIMEOUT=2000 jest --env=node",
    "test-coveralls": "scripts/ci.sh"
  },
  "dependencies": {
    "js-slang": "^0.4.69",
    "lodash": "4.17.20",
    "node-canvas-webgl": "^0.2.6",
    "serverless-offline": "^7.0.0"
  },
  "devDependencies": {
    "@types/jest": "^26.0.15",
    "@types/node": "^14.14.6",
    "coveralls": "^3.1.0",
    "cross-env": "^7.0.2",
    "cross-zip-cli": "^1.0.0",
    "jest": "^26.6.1",
    "ts-jest": "^26.4.3",
    "typescript": "^4.0.5"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/index.ts"
    ],
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "transform": {
      "\\.ts$": "ts-jest"
    },
    "testRegex": "/__tests__/test.*\\.ts$"
  },
  "resolutions": {
    "acorn-walk": "7.2.0"
  }
}
